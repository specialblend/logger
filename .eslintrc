{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
	"@typescript-eslint"
  ],
  "extends": [
	"plugin:@typescript-eslint/eslint-recommended",
	"plugin:@typescript-eslint/recommended",
	"@specialblend/eslint-config"
  ],
  "parserOptions": {
	"ecmaVersion": 6,
	"sourceType": "module",
	"ecmaFeatures": {
	  "modules": true
	}
  },
	"rules": {
		"camelcase": "off"
	}
}
